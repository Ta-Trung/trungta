{
  "name": "trungta",
  "version": "1.0.0",
  "description": "Landing page for Trung Ta",
  "main": "index.js",
  "author": "Trung Ta",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/_s#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/_s.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "concat": "^1.0.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "watch:sass": "node-sass ./assets/sass/main.scss ./assets/css/main.css -w",
    "compile:sass": "node-sass ./assets/sass/main.scss ./assets/css/main.comp.css",
    "concat:css": "concat -o ./assets/css/main.concat.css ./assets/css/all.css ./assets/css/main.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' ./assets/css/main.concat.css -o ./assets/css/main.prefix.css",
    "compress:css": "node-sass ./assets/css/main.prefix.css ./assets/css/main.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  }
}
